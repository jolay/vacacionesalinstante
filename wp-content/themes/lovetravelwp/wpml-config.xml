<wpml-config>
    <custom-fields>
        <custom-field action="translate">lovetravel_btnoffer_travel</custom-field>
        <custom-field action="translate">lovetravel_description_travel</custom-field>
        <custom-field action="copy">lovetravel_duration_travel</custom-field>
        <custom-field action="copy">lovetravel_iframe_travel</custom-field>
        <custom-field action="copy">lovetravel_price_travel</custom-field>
        <custom-field action="translate">lovetravel_textduration_travel</custom-field>
        <custom-field action="translate">lovetravel_parallax_description</custom-field>
        <custom-field action="copy">lovetravel_parallax_image</custom-field>
        <custom-field action="translate">lovetravel_parallax_title</custom-field>
        <custom-field action="copy">lovetravel_main_color</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">travel</custom-type>
    </custom-types>
    <taxonomies>
        <taxonomy translate="1">destination</taxonomy>
        <taxonomy translate="1">typology</taxonomy>
        <taxonomy translate="1">period</taxonomy>
        <taxonomy translate="1">duration</taxonomy>
        <taxonomy translate="1">price</taxonomy>
    </taxonomies>
    <admin-texts>
        <key name="framework">
            <key name="header_titleleftmenu" />
            <key name="header_leftcontenttopnavigation" />
            <key name="header_rightcontenttopnavigation" />
            <key name="travel_searchformaction" />
            <key name="pages_pagenotfoundtitle" />
            <key name="pages_notfoundsearchtitle" />
            <key name="footer_textcopyright" />
        </key>
    </admin-texts>
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>    
        <key name="icl_language_switcher_sidebar">0</key>  
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>    
    </language-switcher-settings>    
</wpml-config>